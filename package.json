{
  "name": "graf_v4",
  "version": "0.12.1",
  "private": true,
  "description": "A graphical app to view and modify files .teq4, teq4Z and .csv",
  "main": "./out/main/index.js",
  "author": {
    "name": "Jose Ivan",
    "email": "jigonzalez930209@gmail.com"
  },
  "license": "MIT",
  "email": "jigonzalez930209@gmail.com",
  "homepage": "https://github.com/jigonzalez930209/graf_v4/releases",
  "build": {
    "appId": "com.jigonzalez930209.graf_v4",
    "asar": true,
    "publish": {
      "provider": "github",
      "owner": "jigonzalez930209",
      "repo": "graf_v4",
      "releaseType": "release"
    },
    "productName": "graf_v4",
    "win": {
      "icon": "resources/graph-icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "artifactName": "${productName}-Windows-${version}-Setup-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": true
    },
    "mac": {
      "target": [
        "dmg",
        "zip",
        "pkg"
      ],
      "artifactName": "${productName}-Mac-${version}-Setup.${ext}"
    },
    "linux": {
      "icon": "resources/graph-icon.png",
      "category": "Office",
      "maintainer": "Jose Ivan <jigonzalez@gmail.com>",
      "description": "Graf is a data visualization tool for the TEQ4",
      "target": [
        "AppImage",
        "deb",
        "tar.gz"
      ],
      "artifactName": "${productName}-Linux-${version}-Setup.${ext}"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:wasm": "cd math-lib && PATH=$HOME/.cargo/bin:$PATH wasm-pack build --target web",
    "build": "pnpm run build:wasm && pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "decimal.js": "^10.6.0",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.6.0",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.21",
    "math-lib": "file:math-lib/pkg",
    "mathjs": "^14.5.3",
    "ml-levenberg-marquardt": "^4.1.3",
    "plotly.js-dist": "^3.0.1",
    "react-dropzone": "^14.3.8",
    "react-plotly.js": "^2.6.0",
    "react-resizable-panels": "^2.1.9",
    "sonner": "^2.0.6",
    "vaul": "^1.1.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@handsontable/react": "^15.3.0",
    "@hookform/resolvers": "^4.1.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.16.2",
    "@types/plotly.js": "^2.35.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-plotly.js": "^2.6.3",
    "@types/sortablejs": "^1.15.8",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "csv-string": "^4.1.1",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "handsontable": "^15.3.0",
    "hyperformula": "^3.0.0",
    "lucide-react": "^0.479.0",
    "next-themes": "^0.4.6",
    "notistack": "^3.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^9.2.0",
    "react-sortablejs": "^6.1.4",
    "react-spinners": "^0.15.0",
    "simple-statistics": "^7.8.8",
    "sortablejs": "^1.15.6",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tinycolor2": "^1.6.0",
    "typescript": "^5.8.3",
    "usehooks-ts": "^3.1.1",
    "vite": "^7.0.4",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.9",
    "wasm-pack": "^0.13.1",
    "zod": "^3.25.76"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "@tailwindcss/oxide",
      "core-js",
      "electron",
      "electron-winstaller",
      "es5-ext",
      "esbuild",
      "sharp",
      "wasm-pack"
    ]
  }
}
